{
  "name": "forms",
  "version": "1.0.0",
  "description": "Provides a quick way to test an HTML form",
  "scripts": {
    "build": "./node_modules/.bin/frontend-dependencies",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sidney Nemzer",
  "repository": "https://github.com/SidneyNemzer/forms",
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.localstorage": "^1.1.16",
    "frontend-dependencies": "^0.2.1",
    "jquery": "^3.1.1",
    "requirejs": "^2.3.3",
    "underscore": "^1.8.3"
  },
  "frontendDependencies": {
    "target": "scripts/lib",
    "packages": [
      "jquery/dist/*",
      "underscore",
      "backbone",
      "backbone.localstorage/*.js",
      "requirejs/*.js"
    ]
  }
}
